@model IEnumerable<ContestApp.Models.ResultatViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Epreuve.Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Profil.Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Temps)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Positionfinale)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Epreuve.Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Profil.Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Temps)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Positionfinale)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
